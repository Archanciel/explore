<KivyCustomClassFields>: # inherits from KivyCustomClassSpinner,
                         # is the lowest sub class.
	orientation: 'vertical'
	TextInput:
		id: first_text_input
		size_hint_y: 0.15
		text: "First TextInput"
		multiline: False
		on_text_validate: root.printFirstText()
	TextInput:
		id: second_text_input
		size_hint_y: 0.15
		text: "Second TextInput"
		multiline: False
		on_text_validate: root.printSecondText()
