# Next import instruction avoids importing the class in the app python file, import
# which will be removed by Pycharm when optimizing the imports !

#: import KivyCustomClassButtons kivycustomclassbuttons.KivyCustomClassButtons

<KivyCustomClassFieldsButtons>: # inherits from BoxLayout
	orientation: 'vertical'
    TextInput:
        id: first_text_input
        size_hint_y: 0.07
        text: "First TextInput"
        multiline: False
        on_text_validate: root.printFirstText()
    TextInput:
        id: second_text_input
        size_hint_y: 0.07
        text: "Second TextInput"
        multiline: False
        on_text_validate: root.printSecondText()
    KivyCustomClassButtons:
        id: kivy_custom_class_buttons